{
  "manifest_version": 2,
  "name": "Autecho",
  "version": "0.0.1",
  "description": "automatic notebook",
  "background" : {
    "scripts" : ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "history",
    "bookmarks",
    "contextMenus"
  ],
  "icons" : {
    "128": "icon.png"
  }
}
